{
  "version": 2,
  "builds": [
    {
      "src": "fuel_route/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "fuel_route/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "fuel_route.settings",
    "DEBUG": "False"
  }
}
